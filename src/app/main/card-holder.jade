form(name="chForm", ng-submit="wizard.goTo('^.cvv')")
  .form-group
    label.control-label Card holder
    input.form-control(ng-model="wizard.cardInfo.cardHolder"
    placeholder="John Doe"
    ng-pattern="wizard.Validate.cardHolderPattern" required)

  .row
    .col-md-3
      button.btn.btn-block(type="submit", ui-sref="^.card-number") Back
    .col-md-3
      button.btn.btn-primary.btn-block(type="submit", ng-disabled="!chForm.$valid") Next
